<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.lang.String?>
<Pane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="scenes.main.MainMenuController" prefHeight="720.0" prefWidth="1280.0">
    <stylesheets>
        <String fx:value="/stylesheet/mainMenu.css"/>
    </stylesheets>
    <StackPane prefHeight="720.0" prefWidth="1280.0">
        <VBox spacing="15" alignment="CENTER" fx:id="vbox">
            <Button fx:id="singleButton" prefWidth="250" prefHeight="50" text="Single Player" onAction="#singlePlayerAction" StackPane.alignment="CENTER"/>
            <Button fx:id="MultiplButton" prefWidth="250" prefHeight="50" text="Multiplayer" onAction="#multipPlayerAction" StackPane.alignment="CENTER"/>
            <Button fx:id="testButton" prefWidth="250" prefHeight="50" text="Test Game" onAction="#gameAction" StackPane.alignment="CENTER"/>
            <Button fx:id="editorButton" prefWidth="250" prefHeight="50" text="Editer" onAction="#editorAction" StackPane.alignment="CENTER"/>
            <Button fx:id="optionButton" prefWidth="250" prefHeight="50" text="Option" onAction="#optionAction" StackPane.alignment="TOP_CENTER"/>
            <Button fx:id="exitButton" prefWidth="250" prefHeight="50" text="Quitter" onAction="#exitAction" StackPane.alignment="BOTTOM_CENTER"/>
        </VBox>
    </StackPane>
    <ToggleButton text="Music On" layoutX="30" layoutY="650" fx:id="musicToggleButton" onMouseClicked="#setMusicToggleButton"/>
</Pane>